{
	"presets": [
		["@babel/preset-env"]
	],
	"plugins": [
		"@babel/plugin-transform-runtime",
		"@babel/proposal-class-properties",
		"@babel/proposal-object-rest-spread",
	],
	"env": {
		"production": {
			"plugins": [
				["transform-remove-console", { "exclude": [ "error" ] }],
			]
		},
		"development": {}
	}
}
